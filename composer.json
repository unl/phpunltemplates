    "autoload": {
        "psr-4": { "UNL\\Templates\\": "src/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/unl/phpdwtparser.git"
        }
    ],
    "require": {
        "php": ">=5.5",
        "unl/php-dwt-parser": "1.0.*"
    },
    "require-dev": {
        "phpunit/PHPUnit": "~4.0",
        "satooshi/php-coveralls": "^0.7.0"
    },
    "autoload-dev": {
        "psr-4": {
            "UNLTest\\Templates\\": "tests/"
        }
    }
}
