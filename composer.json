    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Kevin Abel (kabel)",
            "email": "kabel2@unl.edu",
            "role": "Developer"
        },
        {
            "name": "Brett Bieber (saltybeagle)",
